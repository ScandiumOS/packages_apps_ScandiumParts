<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BubbleToggleView">
        <attr name="scandium_icon" format="reference" />
        <attr name="scandium_iconWidth" format="dimension" />
        <attr name="scandium_iconHeight" format="dimension" />
        <attr name="scandium_shape" format="reference" />
        <attr name="scandium_shapeColor" format="color" />
        <attr name="scandium_showShapeAlways" format="boolean" />
        <attr name="scandium_title" format="string" />
        <attr name="scandium_titleSize" format="dimension" />
        <attr name="scandium_colorActive" format="color" />
        <attr name="scandium_colorInactive" format="color" />
        <attr name="scandium_active" format="boolean" />
        <attr name="scandium_duration" format="integer" />
        <attr name="scandium_padding" format="dimension" />
        <attr name="scandium_titlePadding" format="dimension" />
    </declare-styleable>
    <declare-styleable name="BubbleNavigationConstraintView">
        <attr name="scandium_mode">
            <enum name="spread" value="0" />
            <enum name="inside" value="1" />
            <enum name="packed" value="2" />
        </attr>
    </declare-styleable>
</resources>
