<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BubbleToggleView">
        <attr name="nad_icon" format="reference" />
        <attr name="nad_iconWidth" format="dimension" />
        <attr name="nad_iconHeight" format="dimension" />
        <attr name="nad_shape" format="reference" />
        <attr name="nad_shapeColor" format="color" />
        <attr name="nad_showShapeAlways" format="boolean" />
        <attr name="nad_title" format="string" />
        <attr name="nad_titleSize" format="dimension" />
        <attr name="nad_colorActive" format="color" />
        <attr name="nad_colorInactive" format="color" />
        <attr name="nad_active" format="boolean" />
        <attr name="nad_duration" format="integer" />
        <attr name="nad_padding" format="dimension" />
        <attr name="nad_titlePadding" format="dimension" />
    </declare-styleable>
    <declare-styleable name="BubbleNavigationConstraintView">
        <attr name="nad_mode">
            <enum name="spread" value="0" />
            <enum name="inside" value="1" />
            <enum name="packed" value="2" />
        </attr>
    </declare-styleable>
</resources>
